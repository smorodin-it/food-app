{
  "name": "frontend-router",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/router/src",
  "projectType": "library",
  "tags": ["scope:frontend", "type:router"],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/frontend/router/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/frontend/router"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/frontend/router"
      }
    }
  }
}
